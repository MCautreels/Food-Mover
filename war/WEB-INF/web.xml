<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>FoodMover</servlet-name>
		<servlet-class>org.rhok.foodmover.FoodMoverServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FoodMover</servlet-name>
		<url-pattern>/foodmover</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddListing</servlet-name>
		<servlet-class>org.rhok.foodmover.api.AddListingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddListing</servlet-name>
		<url-pattern>/api/v1/listings</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ListingCreate</servlet-name>
		<servlet-class>org.rhok.foodmover.web.servlets.CreateListingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListingCreate</servlet-name>
		<url-pattern>/createListing</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
